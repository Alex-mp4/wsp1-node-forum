{% extends "layout.njk" %}
{% block  content %}
    {% for row in rows %}
    <div class="posts text-bg-dark">
        <div class="display">
            <img src="img/lostsummer.jpg">
            {{ row.authorId }}
        </div>
        <h1>{{ row.title }}</h1>
        <h3>{{ row.content }}</h3>
    </div>
    <p>Posted at {{ row.createdAt }}</p>
    {% endfor %}
{% endblock %}